<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0"
     xmlns:jwplayer="http://developer.jwplayer.com"
     xmlns:media="http://search.yahoo.com/mrss/">
  <channel>
    <title>TheP HLS Video with VAST Ad</title>
    <item>
      <title>TheP HLS Stream</title>
      <description>JW Player XML example using HLS (.m3u8) and a VAST pre-roll ad</description>

      <!-- HLS stream -->
      <media:content url="https://raw.githubusercontent.com/pothabattulavinod/d/refs/heads/main/z/video1.m3u8"
                     type="application/vnd.apple.mpegurl" />

      <!-- VAST ad schedule -->
      <jwplayer:adSchedule>
        <jwplayer:adBreak time="pre">
          <!-- Public test VAST ad URL -->
          <jwplayer:vastAdTag url="https://playertest.longtailvideo.com/pre-roll-vast.xml" />
        </jwplayer:adBreak>
      </jwplayer:adSchedule>

    </item>
  </channel>
</rss>
